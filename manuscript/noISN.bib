keywords = {Animals,Biodiversity,Ecosystem,Geography,Models, Theoretical,Population Dynamics},
month = jul,
number = {1},
pages = {E1--19},
pmid = {15266376},
title = {{The latitudinal gradient in niche breadth: concepts and evidence.}},
volume = {164},
year = {2004}
}
