month = nov,
number = {11},
pages = {2736--43},
pmid = {18051641},
title = {{Geographical gradients in diet affect population dynamics of Canada lynx.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18051641},
volume = {88},
year = {2007}
}
