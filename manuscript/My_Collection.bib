keywords = {Animals,Ants,Ants: genetics,Ants: parasitology,Ants: physiology,Diptera,Diptera: physiology,Female,Genetic Variation,Host-Parasite Interactions,Immunity, Innate,Immunity, Innate: genetics,Male,Microsatellite Repeats,Sexual Behavior, Animal,Social Behavior},
month = jan,
number = {1606},
pages = {103--10},
pmid = {17035169},
title = {{Increased genetic diversity as a defence against parasites is undermined by social parasites: Microdon mutabilis hoverflies infesting Formica lemani ant colonies.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1679886\&tool=pmcentrez\&rendertype=abstract},
