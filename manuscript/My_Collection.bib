keywords = {Animals,Eukaryota,Eukaryota: physiology,Fishes,Fishes: parasitology,Host-Parasite Interactions,Mammals,Mammals: parasitology,Models, Biological,Phylogeny,Siphonaptera,Siphonaptera: physiology},
month = jul,
number = {7},
pages = {2043--51},
pmid = {18705389},
title = {{High intervality explained by phylogenetic constraints in host-parasite webs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18705389},
volume = {89},
year = {2008}
}
