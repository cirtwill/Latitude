pmid = {12204133},
title = {{Large-scale species-richness gradients in the Atlantic Ocean.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1691087\&tool=pmcentrez\&rendertype=abstract},
volume = {269},
year = {2002}
}
