journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Ants,Ants: genetics,Ants: parasitology,Ants: physiology,Diptera,Diptera: physiology,Female,Genetic Variation,Host-Parasite Interactions,Immunity, Innate,Immunity, Innate: genetics,Male,Microsatellite Repeats,Sexual Behavior, Animal,Social Behavior},
month = jan,
