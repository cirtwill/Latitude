journal = {Molecular Biology and Evolution},
keywords = {address correspondence,confidence limit tree,hidetoshi shimodaira,ings,kishino hasegawa,mammalian phylogeny,multiple comparisons,multiplicity test,reprints,selection bias,test,topology},
number = {8},
pages = {1114--1116},
pmid = {19864619},
title = {{Letter to the Editor Multiple Comparisons of Log-Likelihoods with Applications to Phylogenetic Inference}},
volume = {16},
year = {1999}
}
