journal = {Ecology},
keywords = {community,competition,dom-,dynamics,have argued that stochastic,larval supply is the,predation,recruitment},
number = {10},
pages = {2872--2880},
title = {{Priority Effects in Coral Reef Fish Communities of the Great Barrier Reef}},
volume = {85},
year = {2004}
}
