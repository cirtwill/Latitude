@article{Vuong1989,
abstract = {In this paper, we develop a classical approach to model selection.$\backslash$nUsing the Kullback-Leibler Information Criterion to measure the closeness$\backslash$nof a model to the truth, we propose simple likelihood-ratio based$\backslash$nstatistics for testing the null hypothesis that the competing models$\backslash$nare equally close to the true data generating process against the$\backslash$nalternative hypothesis that one model is closer. The tests are directional$\backslash$nand are derived successively for the cases where the competing models$\backslash$nare non-nested, overlapping, or nested and whether both, one, or$\backslash$nneither is misspecified. As a prerequisite, we fully characterize$\backslash$nthe asymptotic distribution of the likelihood ratio statistic under$\backslash$nthe most general conditions. We show that it is a weighted sum of$\backslash$nchi-square distribution or a normal distribution depending on whether$\backslash$nthe distributions in the competing models closest to the truth are$\backslash$nobservationally identical. We also propose a test of this latter$\backslash$ncondition.},
author = {Vuong, Q H},
doi = {10.2307/1912557},
file = {:home/arc136/Desktop/Papers/Vuong - 1989 - Econometrica.pdf:pdf},
isbn = {0012-9682},
issn = {00129682},
journal = {Econometrica},
keywords = {likelihood ratio tests,misspecified,model selection,models,non-nested hypotheses,weighted sums of chi-squares},
