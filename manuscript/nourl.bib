journal = {Journal of the New York Entomological Society},
keywords = {http://www.archive.org/details/journalofnewyork121},
number = {3},
pages = {158--165},
title = {{Insect succession in the decomposition of a mammal in Costa Rica}},
volume = {89},
year = {1981}
}
