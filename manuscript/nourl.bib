keywords = {Binary food web,Lake Cis\'{o},Prey refuge,Trophic food web},
number = {3},
pages = {279--288},
pmid = {7070},
title = {{Components, structure and fluxes of the microbial food web in a small, stratified lake}},
volume = {11},
year = {1996}
}
