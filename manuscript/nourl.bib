journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Algorithms,Cluster Analysis,Economic,Escherichia coli,Escherichia coli: metabolism,Metabolic Networks and Pathways,Metabolism,Models,Probability,Social Class,Statistical,Systems Biology,Systems Theory,Theoretical,Transportation},
