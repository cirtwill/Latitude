journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Anthelmintics,Anthelmintics: pharmacology,Bartonella,Bartonella: physiology,Biota,Eimeria,Eimeria: physiology,England,Host-Parasite Interactions,Ivermectin,Ivermectin: pharmacology,Murinae,Murinae: parasitology,Nematoda,Nematoda: drug effects,Nematoda: physiology,Population Dynamics,Trypanosoma,Trypanosoma: physiology},
number = {1762},
pages = {20130598},
pmid = {23677343},
title = {{Stability of within-host-parasite communities in a wild mammal system.}},
volume = {280},
year = {2013}
}
